<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AirQualityHybrid.Pages.DetailsPage"
             xmlns:components="clr-namespace:AirQualityWeb.SharedComponents;assembly=AirQualityWeb.SharedComponents"
             Title="Air Quality Details"
             Padding="10">
    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent x:Name="MyComponent" Selector="#app" ComponentType="{x:Type components:LocationDetails}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</ContentPage>